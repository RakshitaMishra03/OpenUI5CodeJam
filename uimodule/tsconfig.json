{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "rootDir": "./webapp",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "uimodule/*": [
        "./webapp/*"
      ]
    },
    "types": [
      "@sapui5/types",
      "@types/qunit"
    ]
  },
  "include": [
    "./webapp/**/*",
    "./.ui5-tooling-modules/types/**/*"
  ],
  "exclude": [
    "./webapp/test/e2e/**/*"
  ]
}